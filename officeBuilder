local function placer(numBlocks)
  for i = 1, numBlocks do
    turtle.forward()
    turtle.placeDown()
  end
end

local function adjust(directions)
  if directions == "R" then
    turtle.turnRight()
    turtle.forward()
    turtle.turnLeft()
  elseif directions == "L" then
    turtle.turnLeft()
    turtle.forward()
    turtle.turnRight()
  elseif directions == "FR" then
    turtle.forward()
    turtle.turnRight()
  elseif directions == "FL" then
    turtle.forward()
    turtle.turnLeft()
  end
end

local function main()
  placer(3)
  adjust("L")
  placer(2)
  adjust("L")
  placer(2)
  adjust("L")
  placer(1)
  adjust("L")
  placer(1)
  adjust("L")
  placer(1)
  adjust("L")
  placer(1)
  adjust("L")
  placer(1)
  adjust("FL")
  placer(2)
  adjust("R")
  placer(2)
  adjust("R")
  placer(3)
  adjust("FR")
  placer(10)
  adjust("R")
  
  placer(3)
  adjust("R")
  placer(2)
  adjust("R")
  placer(2)
  adjust("R")
  placer(1)
  adjust("R")
  placer(1)
  adjust("R")
  placer(1)
  adjust("R")
  placer(1)
  adjust("R")
  placer(1)
  adjust("FR")
  placer(2)
  adjust("L")
  placer(2)
  adjust("L")
  placer(3)
  adjust("L")
  placer(10)
  adjust("R")
  
  placer(3)
  adjust("R")
  placer(2)
  adjust("R")
  placer(2)
  adjust("R")
  placer(1)
  adjust("R")
  placer(1)
  adjust("R")
  placer(1)
  adjust("R")
  placer(1)
  adjust("R")
  placer(1)
  adjust("FR")
  placer(2)
  adjust("L")
  placer(2)
  adjust("L")
  placer(3)
  adjust("L")
  placer(10)
  adjust("FR")
  
  placer(3)
  adjust("L")
  placer(2)
  adjust("L")
  placer(2)
  adjust("L")
  placer(1)
  adjust("L")
  placer(1)
  adjust("L")
  placer(1)
  adjust("L")
  placer(1)
  adjust("L")
  placer(1)
  adjust("FL")
  placer(2)
  adjust("R")
  placer(2)
  adjust("R")
  placer(3)
  adjust("FR")
  placer(10)
  adjust("R")
end

main()
