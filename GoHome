os.loadAPI("moveAPI")

moveAPI.locate()

while not moveAPI( moveAPI.getX(), 255, moveAPI.getZ(), { "y","x","z" } ) do
  turtle.digUp()
  turtle.attackUp()
end

while not moveAPI( 205, 255, 460, { "x", "z", "y" } ) do
  turtle.dig()
  turtle.attack()
end

while not moveAPI( 205, 70, 460, { "x", "z", "y" } ) do
  turtle.digDown()
  turtle.attackDown()
end

print("I am drunk.")
